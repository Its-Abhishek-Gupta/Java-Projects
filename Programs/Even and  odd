import java.util.Scanner;
public class Add {
    public static void main (String[] args){
        Scanner i = new Scanner(System.in);

        System.out.println("Input first number");
        float a = i.nextFloat();

        if (a%2==0){
            System.out.println(a+" is even");
        }
        else{
            System.out.println(a+" is odd");
        }
    }
}
