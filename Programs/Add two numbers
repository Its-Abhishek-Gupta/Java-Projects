import java.util.Scanner;
public class Add {
    public static void main (String[] args){
        //Input two numbers
        Scanner i = new Scanner(System.in);
        System.out.println("Input a and b");
        float a = i.nextFloat();
        float b = i.nextFloat();
        float c = a+b;
        System.out.println("Sum of a and b = "+c);
    }
}
